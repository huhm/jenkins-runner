{
  "name": "jenkins-runner",
  "version": "1.0.8",
  "description": "a simple jenkins runner cli and apis",
  "main": "lib/index.js",
  "bin": {
    "jenkins-runner": "./bin/jenkins-runner.js"
  },
  "homepage": "https://github.com/huhm/jenkins-runner",
  "scripts": {
    "build": "tsc",
    "release": "tsc && npm publish",
    "dev:example": "node example/jenkins_job_caller.js",
    "dev:help": "tsc && node ./bin/jenkins-runner -h",
    "dev:init": "tsc && node ./bin/jenkins-runner init -d local",
    "dev:test-runner": "tsc && node ./bin/jenkins-runner  -c ./local/jenkins_runner.config.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huhm/jenkins-runner.git"
  },
  "author": "huhuimei2008@gmail.com",
  "keywords": [
    "jenkins",
    "jenkins-runner cli"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.4",
    "chalk": "^4.1.2",
    "commander": "^8.2.0",
    "crypto-js": "^4.1.1",
    "jenkins": "^0.28.1"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/crypto-js": "^4.0.2",
    "@types/jenkins": "^0.23.2",
    "cross-env": "^7.0.3"
  }
}